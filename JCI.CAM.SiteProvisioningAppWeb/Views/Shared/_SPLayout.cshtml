<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <meta name="description" content="Site provisioning request" />
</head>
<body>

    <div class="container body-content">
        @RenderBody()
    </div>

    @RenderSection("Variables", true)
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/Scripts/js-webshim/dev/polyfiller.js")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/knockout")
    @Scripts.Render("~/bundles/JCI-CAM")
    @Scripts.Render("~/bundles/AzWizard")
    @Scripts.Render("~/scripts/peoplepickercontrol.js")

    @RenderSection("scripts", required: false)
</body>
</html>
